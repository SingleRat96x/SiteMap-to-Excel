{
  "name": "sitemap-to-xlsx",
  "version": "1.0.0",
  "description": "Chrome extension to convert sitemaps to XLSX files",
  "scripts": {
    "build": "webpack --config webpack.config.js && node scripts/convert-icons.js",
    "watch": "webpack --config webpack.config.js --watch",
    "dev": "webpack serve --config webpack.config.js",
    "icons": "node scripts/convert-icons.js"
  },
  "dependencies": {
    "@material-tailwind/react": "^2.1.8",
    "pako": "^2.1.0",
    "xlsx": "^0.18.5",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.254",
    "@types/pako": "^2.0.3",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/xml2js": "^0.4.14",
    "autoprefixer": "^10.4.16",
    "buffer": "^6.0.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "postcss": "^8.4.32",
    "postcss-loader": "^7.3.3",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.33.1",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.4.0",
    "timers-browserify": "^2.0.12",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "util": "^0.12.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
